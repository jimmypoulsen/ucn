package model;

/**
 * Music track
 * 
 * @author fen, mars, knol, mhi
 * @version 2003.10.27, 2009.29.09, 2015.11.01, 2018.10.19
 */
public class Track {
    private int length;
    private String title;

    /**
     * Constructor for objects of class Nummer
     */
    public Track(String title, int length) {
        this.length= length;
        this.title= title;
    }

    public int getLength() {
        return length;
    }

    public String getTitle() {
        return title;
    }
}